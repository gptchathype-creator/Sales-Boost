{
  "name": "sales-boost-bot",
  "version": "1.0.0",
  "description": "Telegram bot for evaluating sales managers' expertise",
  "main": "dist/src/index.js",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && vite build --config admin-frontend/vite.config.ts",
    "start": "node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "regenerate": "tsx scripts/regenerate-evaluations.ts",
    "backfill-training": "tsx scripts/backfill-training-assessments.ts",
    "generate-mock": "tsx scripts/generate-mock-data.ts",
    "delete-mock": "tsx scripts/delete-mock-data.ts",
    "check-openai": "tsx scripts/check-openai.ts",
    "tunnel": "lt --port 3000 --subdomain salesboost",
    "test": "vitest run",
    "test:watch": "vitest",
    "call-test:build": "npm run build -w @smoke/voximplant-smoke && npm run build -w @smoke/vox-smoke-test-server",
    "call-test:dev": "npm run dev -w @smoke/vox-smoke-test-server",
    "call-test:stats": "npm run stats -w @smoke/vox-smoke-test-server"
  },
  "keywords": [
    "telegram",
    "bot",
    "sales",
    "evaluation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@heroui/react": "^2.8.9",
    "@prisma/client": "^5.7.1",
    "@tailwindcss/postcss": "^4.2.1",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "autoprefixer": "^10.4.24",
    "cloudflared": "^0.7.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.34.3",
    "localtunnel": "^2.0.2",
    "openai": "^4.20.1",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.2.1",
    "telegraf": "^4.15.4",
    "undici": "^7.20.0",
    "vite": "^7.3.1",
    "ws": "^8.19.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.18.1",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
