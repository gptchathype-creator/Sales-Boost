{
  "name": "sales-boost-bot",
  "version": "1.0.0",
  "description": "Telegram bot for evaluating sales managers' expertise",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "regenerate": "tsx scripts/regenerate-evaluations.ts",
    "backfill-training": "tsx scripts/backfill-training-assessments.ts",
    "generate-mock": "tsx scripts/generate-mock-data.ts",
    "delete-mock": "tsx scripts/delete-mock-data.ts",
    "check-openai": "tsx scripts/check-openai.ts",
    "tunnel": "lt --port 3000 --subdomain salesboost"
  },
  "keywords": [
    "telegram",
    "bot",
    "sales",
    "evaluation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cloudflared": "^0.7.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "localtunnel": "^2.0.2",
    "openai": "^4.20.1",
    "telegraf": "^4.15.4",
    "undici": "^7.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
